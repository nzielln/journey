<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000"
        motion:autoTransition="animateToEnd">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/compass"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="376dp"
            android:layout_height="564dp"
            android:layout_marginBottom="5dp"
            motion:layout_constraintBottom_toTopOf="@+id/journeyTypeface"
            motion:layout_constraintVertical_bias="0.0"
            motion:layout_constraintHorizontal_bias="0.5"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="20dp"
            android:rotation="270" />
        <Constraint
            android:id="@+id/finalLogo"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="674dp"
            android:layout_height="979dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="24dp"
            motion:layout_constraintHorizontal_bias="0.503"
            motion:layout_constraintStart_toStartOf="parent"
            android:visibility="invisible" />
        <Constraint
            android:id="@+id/journeyTypeface"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="396dp"
            android:layout_height="178dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="45dp"
            motion:layout_constraintVertical_bias="1.0"
            motion:layout_constraintHorizontal_bias="0.5"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="400dp"
            android:visibility="invisible" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/compass"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="376dp"
            android:layout_height="564dp"
            android:layout_marginBottom="5dp"
            motion:layout_constraintBottom_toTopOf="@+id/journeyTypeface"
            motion:layout_constraintVertical_bias="1.0"
            motion:layout_constraintHorizontal_bias="0.941"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="20dp"
            android:visibility="invisible" />
        <Constraint
            android:id="@+id/journeyTypeface"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="396dp"
            android:layout_height="178dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="45dp"
            motion:layout_constraintVertical_bias="1.0"
            motion:layout_constraintHorizontal_bias="0.5"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="400dp"
            android:visibility="invisible" />
        <Constraint
            android:id="@+id/finalLogo"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="674dp"
            android:layout_height="979dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="24dp"
            motion:layout_constraintHorizontal_bias="0.503"
            motion:layout_constraintStart_toStartOf="parent"
            motion:transitionEasing="cubic(0.96,0.15,0.8,0.8)"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="-10dp" />
    </ConstraintSet>
</MotionScene>